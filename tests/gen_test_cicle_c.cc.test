#include <iostream>

using std::cout;
using std::endl;

#define NUM_OF_LOOPS 10

int
main()
{
  cout << "The val of NUM_OF_LOOPS is ";
  cout << 10 << endl;

  cout << "These are the numbers from " << 
          1 << " to " << 10 << endl;
  
  for (int i = 1; i <= 10; ++i)
     cout << i << " ";
  
  cout << endl;

  return 0;
}
